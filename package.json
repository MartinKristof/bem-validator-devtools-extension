{
  "name": "bem-validator-extension",
  "version": "0.1.0",
  "description": "A DevTools extension for inspecting CSS and validate for BEM",
  "scripts": {
    "build": "webpack",
    "dev": "webpack --watch",
    "ci": "run-s test lint pretty:check",
    "test": "npm run jest:coverage",
    "jest": "jest --config jest.config.json",
    "jest:watch": "npm run jest -- --watch",
    "jest:coverage": "npm run jest -- --coverage",
    "jest:update": "npm run jest -- -u",
    "jest:only-changed": "npm run jest -- -o",
    "pretty": "prettier --write '{src,style,web-extension,tests}/**/*'",
    "pretty:check": "prettier --list-different '{src,style,web-extension,tests}/**/*'",
    "lint": "run-s lint:scripts lint:styles",
    "lint:scripts": "eslint src/ web-extension/ tests/",
    "lint:styles": "stylelint '**/*.scss'",
    "fix:scripts": "eslint src/ web-extension/ tests/ --fix",
    "fix:styles": "stylelint '**/*.scss' --fix",
    "generate-icons": "svg-icon-generate --svgDir ./assets/svg --destination ./src/ui/redux/components/icons/Icon.js --keepFillColor true --reactPureRender true"
  },
  "author": "Martin Kristof",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "bootstrap": "^4.2.1",
    "css-loader": "^0.23.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^0.9.0",
    "jest": "^23.6.0",
    "jest-junit": "^6.2.1",
    "jest-webextension-mock": "^3.5.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.3",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.13.1",
    "stylelint": "^9.10.1",
    "stylelint-scss": "^3.5.2",
    "webpack": "^1.13.1",
    "webpack-create-vendor-chunk": "^0.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "css": "^2.2.4",
    "css-should-plugin-bem": "^2.0.0",
    "lodash": "^3.10.1",
    "prop-types": "^15.6.2",
    "radium": "^0.25.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.3",
    "react-redux": "^6.0.0",
    "react-svg-icon-generator": "^0.5.1",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "uuid": "~2.0.1"
  }
}
